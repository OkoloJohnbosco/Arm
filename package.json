{
	"name": "armengage-web",
	"version": "0.1.0",
	"description": "The web app of armengage",
	"scripts": {
		"detect-unused": "node ./scripts/detect-unused.js",
		"analyze-build": "cross-env ANALYZE=true next build",
		"name-export": "npx @next/codemod name-default-component .",
		"dependency-check": "madge  --ts-config ./tsconfig.json --basedir . --extensions js,jsx,ts,tsx --circular ./",
		"dependency-warning": "madge  --image ./dependency-check.svg  --ts-config ./tsconfig.json --basedir . --extensions js,jsx,ts,tsx --warning ./*",
		"dev": "cross-env NODE_ENV=development yarn create-env && next -p 4000",
		"dev:prod": "cross-env NODE_ENV=production yarn create-env && next",
		"dev:staging": "cross-env NODE_ENV=staging yarn create-env && next",
		"debug": "NODE_OPTIONS='--inspect' next dev",
		"build:dev": "next build",
		"build-old": "yarn type-check && yarn run dependency-check && next build",
		"build": "yarn type-check && next build",
		"export": "next build && next export",
		"heroku-postbuild": "yarn run build",
		"start:heroku": "next start -p $PORT",
		"start": "next start -p $PORT",
		"start:dev": "cross-env NODE_ENV=development yarn create-env && next start",
		"start:stagging": "cross-env NODE_ENV=stagging yarn create-env && next start",
		"start:prod": "cross-env NODE_ENV=production yarn create-env && next start",
		"launch": "next build && next start",
		"postinstall.bak": "next build",
		"create-env": "node ./scripts/saveEnvironment.js",
		"type-check": "tsc --noEmit",
		"type-coverage": "type-coverage --strict --at-least 95",
		"prettier": "prettier --write \".//**/*.{ts,tsx,js,json}\"",
		"dedupe": "yarn run yarn-deduplicate && yarn install",
		"dedupe-check": "yarn run yarn-deduplicate --fail",
		"gen:theme-typings": "chakra-cli tokens ./lib/theme/theme.ts",
		"postinstall": "yarn run gen:theme-typings",
		"lintss": "next lint"
	},
	"engines": {
		"yarn": "^1.22"
	},
	"repository": {
		"type": "git",
		"url": "https://rhicstech@dev.azure.com/rhicstech/armengage"
	},
	"keywords": [
		"arm",
		"engage",
		"armengage"
	],
	"author": "Rhics Technology",
	"license": "ISC",
	"dependencies": {
		"@chakra-ui/react": "^1.6.5",
		"@emotion/react": "^11",
		"@emotion/styled": "^11",
		"@lottiefiles/react-lottie-player": "^3.0.0",
		"@sumsub/websdk": "^1.0.6",
		"@types/jquery": "^3.5.5",
		"apexcharts": "^3.23.0",
		"axios": "^0.21.0",
		"broadcast-channel": "^3.3.0",
		"chart.js": "^3.2.1",
		"chartjs-plugin-datalabels": "^1.0.0",
		"chartjs-plugin-doughnutlabel": "^2.0.3",
		"date-fns": "^2.16.1",
		"dotenv": "^8.2.0",
		"framer-motion": "^4",
		"html-react-parser": "^1.0.0",
		"husky": "^4.3.6",
		"jquery": "^3.6.0",
		"lint-staged": "^10.5.3",
		"loaders.css": "^0.1.2",
		"lodash": "^4.17.15",
		"next": "^11.0.1",
		"next-i18next": "^7.0.1",
		"nprogress": "^0.2.0",
		"react": "^17.0.2",
		"react-apexcharts": "^1.3.7",
		"react-chartjs-2": "^3.0.3",
		"react-datepicker": "^3.2.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.2.0",
		"react-loaders": "^3.0.1",
		"react-query": "^3.4.0",
		"react-scroll": "^1.8.1",
		"react-spinners": "^0.10.4",
		"react-wow": "^1.0.0",
		"simple-swizzle": "^0.2.2",
		"styled-components": "^5.1.0",
		"uniqid": "^5.3.0",
		"validator": "^13.5.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@chakra-ui/cli": "^1.3.0",
		"@emotion/babel-plugin": "^11.1.2",
		"@next/bundle-analyzer": "^10.0.3",
		"@svgr/webpack": "^5.5.0",
		"@types/node": "^14.14.20",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-scroll": "^1.8.2",
		"@types/styled-components": "^5.1.7",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"autoprefixer": "10.1.0",
		"babel-plugin-import": "^1.7.0",
		"babel-plugin-inline-react-svg": "^1.1.1",
		"babel-plugin-named-asset-import": "^0.3.5",
		"babel-plugin-root-import": "^6.4.1",
		"babel-plugin-styled-components": "^1.11.1",
		"babel-preset-react-app": "^10.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.16.0",
		"eslint-config-next": "^11.0.1",
		"eslint-plugin-react": "^7.21.5",
		"install": "^0.13.0",
		"madge": "^3.9.2",
		"npm": "^6.14.11",
		"postcss": "8.2.1",
		"prettier": "^2.2.1",
		"sass": "^1.30.0",
		"tailwindcss": "^2.0.2",
		"ts-node": "^9.1.1",
		"tslint": "^6.1.3",
		"typescript": "^4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn run type-check && yarn run dependency-check"
		}
	},
	"lint-staged": {
		"**/*.{ts,js,tsx,json,md}": [
			"yarn run prettier"
		]
	}
}
